{
  "name": "nuxt-ts-example",
  "version": "1.0.0",
  "private": true,
  "description": "Nuxt typescript proejct using nuxt-ts",
  "keywords": [
    "nuxt",
    "typescript"
  ],
  "author": "Taketo Yoshida",
  "license": "MIT",
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate && yarn build:lambda",
    "lint": "tslint --project .",
    "post-update": "yarn upgrade --latest",
    "start:lambda": "netlify-lambda serve ./functions",
    "build:lambda": "netlify-lambda build ./functions"
  },
  "dependencies": {
    "nuxt-ts-edge": "latest",
    "vue-property-decorator": "^7.3.0",
    "vuex-class": "^0.3.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.1.0",
    "@types/aws-lambda": "^8.10.18",
    "@types/node": "^10.12.21",
    "netlify-lambda": "^1.3.3",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.3.1"
  }
}
